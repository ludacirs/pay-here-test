{
  "compilerOptions": {
    "target": "es2016",
    // "lib": [],
    "jsx": "react",
    "module": "commonjs",
    "baseUrl": "./",

    "isolatedModules": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,

    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "skipLibCheck": true
  }
}
